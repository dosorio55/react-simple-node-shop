{
    "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "lib": ["dom", "dom.iterable", "esnext"],
      "allowJs": true,
      "jsx": "react",
      "esModuleInterop": true,
      "strict": true,
      "noImplicitAny": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noEmit": true
    },
    "include": ["src/**/*.tsx", "src/**/*.ts", "src/App.js"],
    "exclude": ["node_modules"]
  }
  